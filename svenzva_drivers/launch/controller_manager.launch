<launch>
    <node name="dynamixel_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                pan_tilt_port:
                    port_name: "/dev/ttyUSB0"
                    baud_rate: 57142
                    min_motor_id: 0
                    max_motor_id: 10
                    update_rate: 20
        </rosparam>
    </node>

    <include file="$(find svenzva_drivers)/launch/start_tilt_controller.launch"/>
</launch>
